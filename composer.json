{
    "name": "theodorejb/iis-log-parser",
    "description": "Parse IIS log files",
    "type": "library",
    "license": "MIT",
    "authors": [
      {
        "name": "Theodore Brown",
        "email": "theodorejb@outlook.com"
      }
    ],
    "require": {
        "php": ">=8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "psalm/plugin-phpunit": "^0.19.5",
        "vimeo/psalm": "^6.13"
    },
    "autoload": {
        "psr-4": {
            "theodorejb\\IISLogParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "theodorejb\\IISLogParser\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "analyze": "psalm",
        "test": "phpunit tests"
    }
}
